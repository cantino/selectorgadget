{
  "name": "selectorgadget",
  "version": "1.0.0",
  "description": "[SelectorGadget](http://www.selectorgadget.com) is an open source bookmarklet that makes CSS selector generation and discovery on complicated sites a breeze.",
  "main": "./lib/js/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jardix22/selectorgadget.git"
  },
  "author": "Joseph Ramos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jardix22/selectorgadget/issues"
  },
  "homepage": "https://github.com/jardix22/selectorgadget",
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "diff_match_patch": "./bower_components/google-diff-match-patch-js/diff_match_patch_uncompressed.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "diff_match_patch": "diff_match_patch"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "browserify-shim": "^3.8.0",
    "coffeeify": "^0.7.0",
    "gulp": "^3.8.10",
    "gulp-sass": "^1.2.2",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.1",
    "vinyl-source-stream": "^1.0.0"
  }
}
